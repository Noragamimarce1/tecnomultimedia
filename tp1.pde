void setup(){
  size(600,600);
  background(255);
}
void draw(){
  strokeWeight(10);
  line(100,180,100,400);
  line(280,100,100,200);
  line(280,80,500,200);
  line(280,80,500,200);
  line(500,200,500,420);
  line(500,400,330,500);
  line(300,510,100,400);
  noStroke();
  //ellipse rojo
  fill(255,3,11);
  ellipse(300,100,100,100);
  
  //ellipse azul
   fill(0,0,255);
  ellipse(500,400,100,100); 
  
  //elipse verde
  fill(0,255,0);
  ellipse(100,400,100,100);
  
  //elipse magenta
  fill(255,0,255);
  ellipse(500,200,100,100);  
  
  //elipse amarillo
  fill(255,255,0);
  ellipse(100,200,100,100);
  
  //elipse cian
  fill(0,255,255);
  ellipse(300,520,100,100);
  //verde amarillento
  fill(100,255,0);
  ellipse(100,300,50,50);
  //elllipse naranja
  fill(255,100,0);
  ellipse(200,150,50,50);
  //elipse coral
  fill(255,0,100);
  ellipse(400,150,50,50);
  //elipse violeta
  fill(100,0,255);
  ellipse(500,300,50,50);
  //alipse ceruleo
  fill(0,100,255);
  ellipse(400,460,50,50);
  //ellipse verde azulado
  fill(0,255,100);
  ellipse(200,460,50,50);
  
  
}
